#include <bits/stdc++.h>

using namespace std;

int main()
{
    int long long n,i,d,s,nr=1;
    cin>>n;
    for(i=2;nr<=n;i++)
    {   
        s=1;
        for(d=2;d*d<i;d++)
        {
            if(i%d==0)
            {
                s+=d;
                s+=i/d;
            }
        }
        if(s==i)
        {
            cout<<i<<" ";
            nr++;
        }
    }
    
    
    
    return 0;
}
