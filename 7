#include <bits/stdc++.h>

using namespace std;

int main()
{
    int long long n,i,d,s,nr=0,y;
    cin>>n;
    for(i=1;i<=n;i++)
    {   
        cin>>y;
        s=1;
        for(d=2;d*d<y;d++)
        {
            if(y%d==0)
            {
                s+=d;
                s+=y/d;
            }
        }
        if(s==y)
        {
            nr++;
        }
    }
    
    cout<<nr;
    
    return 0;
}
